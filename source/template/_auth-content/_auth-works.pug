.title-wrap.panel__title-wrap
	h2.panel__title Страница "Мои работы"
form.panel__form.blog__panel-form
	.panel__form-content
		.panel__form-title Добавить запись
		.panel__form-input-wrap.panel__form-input-wrap_blog-mods
			input.tabs__input(type="text" id="panel__form-work-name", name="project-name" placeholder="Название проекта")
		.panel__form-input-wrap.panel__form-input-wrap_blog-mods
			input.tabs__input(type="text" id="panel__form-work-tech", name="project-tech" placeholder="Технологии")
		.panel__form-input-wrap.panel__form-input-wrap_blog-mods
			label.file-input
				.file-input__fake
					.file-input__icon
						img(src="assets/img/file-input-icon.png" alt="icon")
					.file-input__text Загрузить картинку
				input.tabs__input(name="project-download" type="file")
	.panel__form-submit
		input.panel__form-btn(type="submit" value="Добавить")