nav.nav
	-
		var icons = [
			{ name: "vkontakte",   url: "https://vk.com/", mods: "#vk"},
			{ name: "github", url: "https://github.com/VivereMemento/dz-1", mods: "#github"},
			{ name: "instagram",    url: "https://www.instagram.com/", mods: "#in"}
		]
	ul.social.nav__social_mods
		each icon in icons
			li.social__item
				a(href=icon.url, target="_blank" class="social__link social__link_") #{icon.name}
					svg.icons.header__icons_mods
						use(xlink:href="assets/img/sprite.svg" + icon.mods)

	//social__list

	-var triggers = ["trigger-menu_1", "trigger-menu_2", "trigger-menu_3"]
		.trigger-menu-wrap
			each trigger in triggers
				div(class="trigger-menu " + trigger) 
		//trigger-menu

	.menu
		.menu-wrap
			-var items = ["Мои работы", "Обо мне", "Блог", "Авторизация"]
			ul.menu__list
				each item in items
					li(class="menu__item")
						a(href="#").menu__link=item
	//menu
//nav